<div class="my-4">
  <div class="mx-3 ">
  <h2>Current Users</h2>
  </div>
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Delete</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
          <tr>
            <td>{{firstName}} {{lastName}}</td>
            <td>{{email}}</td>
            <td><a href="admin/deleteuser/{{_id}}" ><i class="fas fa-trash-alt align-middle"></i></a></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div> 
</div>