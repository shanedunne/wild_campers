<div class="my-4">
  <h2>Current Users</h2>
  <div class="list-group">
    {{#each users}}
      <div class="list-group-item d-flex justify-content-between align-items-center">
        <p>ID: {{_id}}</p>
        <p>Name: {{firstName}} {{lastName}}</p>
        <p>Email: {{email}}</p>
        <a href="admin/deleteuser/{{_id}}"><i class="fas fa-trash-alt"></i></a>
      </div>

    {{/each}}
  </div>

</div>